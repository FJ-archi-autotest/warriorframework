<?xml version="1.0" ?>
<Testcase>
        <Details>
			<Name>selenium_demo</Name>
			<Title>Test case to perform Search</Title>
			<ExpectedResults>PASS</ExpectedResults>
			<Category>search</Category>
			<Engineer>Sanika Kulkarni</Engineer>
			<Date/>
			<Time/>
			<default_onError action = 'next'  />
			<InputDataFile>../Data/selenium_demo_Data.xml</InputDataFile>
			<Datatype>Custom</Datatype>
			<Logsdir/>
			<Resultsdir/>
        </Details>
       <Requirements>
       			<Requirement>requirement-001</Requirement>
       			<Requirement>requirement-002</Requirement>
       </Requirements>
        <Steps>
			<step TS="1" Driver="demo_driver" Keyword="create_jira_issue">
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="server_url" value="https://warriorproject.atlassian.net/"/>
					<argument name="username" value="brian.jishi@us.fujitsu.com"/>
					<argument name="password" value="qwerty123"/>
					<argument name="issue_summary" value="Issue created by Warrior Testcase"/>
					<argument name="issue_description" value="This issues was created by the Rest and Selenium Testcase"/>
					<argument name="project_key" value="YFP"/>
				</Arguments>
			</step>
			<step TS= '2' Driver='selenium_driver' Keyword='browser_launch' > <!-- Will launch or open a new browser in window -->
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="url" value="https://warriorproject.atlassian.net/login"/>
				</Arguments>
			</step>
			<step TS= '3' Driver='selenium_driver' Keyword='browser_maximize' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
				</Arguments>
			</step>
			<step TS= '4' Driver='selenium_driver' Keyword='type_text' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="locator_type" value="name"/>
					<argument name="locator" value="username"/>
					<argument name="text" value="brian.jishi@us.fujitsu.com"/>
				</Arguments>
			</step>
			<step TS= '5' Driver='selenium_driver' Keyword='type_text' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="locator_type" value="name"/>
					<argument name="locator" value="password"/>
					<argument name="text" value="qwerty123"/>
				</Arguments>
			</step>
			<step TS= '6' Driver='selenium_driver' Keyword='click_an_element' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="locator_type" value="id"/>
					<argument name="locator" value="login"/>
				</Arguments>
			</step>
			<step TS= '7' Driver='common_driver' Keyword='wait_for_timeout' >
				<Arguments>
					<argument name="timeout" value="10"/>
				</Arguments>
			</step>
			<step TS= '8' Driver='selenium_driver' Keyword='browser_close' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
				</Arguments>
			</step>
		</Steps>
</Testcase>

<?xml version="1.0" ?>
<Testcase>
        <Details>
                <Name>Name</Name> <!-- Required. Testcase name or ID -->
                <Title>Title</Title> <!-- Required. Testcase title or Description -->
				<ExpectedResults>ExpectedResults</ExpectedResults> <!--optional -->
				<Category>Feature</Category> <!-- optional -->
                <Engineer></Engineer> <!--Required -->
				<Date></Date>
				<Time></Time>
			    <default_onError action = 'goto' value='9'  /><!--Optional --><!--
				action = next, abort, goto, abort_as_error
				value = used with action goto to specify step number 
				Default Error Handling should be a drop down with default value of 'next' 
				 -->
				<InputDataFile>Path_to_InputDataFile</InputDataFile> <!-- 
				Optional. The user shall be able to select a data file from a local machine. Relative Path is supported.
				-->
				<Datatype>Datatype</Datatype><!-- Required if using InputDataFile Options: 'Iterative, Custom'
				Iterative: Expects a system based input data file. Will run the keywords on every system defined.
				Custom : Allows for a custom defined data file, with custome variables assigned to keyword to 
				access the input data file-->
				<Logsdir></Logsdir> <!--Optional, uses default-->
				<Resultsdir></Resultsdir> <!--Optional, uses default-->
        </Details>
       <Requirements> <!-- The user shall be able to add as many requirements as needed-->
       			<Requirement>requirement-001</Requirement> <!--Optional -->
       			<Requirement>requirement-002</Requirement>
       </Requirements>
        <Steps> <!-- The user shall be able to add as many steps as needed. At least one is required-->
				<step TS= '1' Driver='DriverName'      Keyword='KeywordName' > 
					<Description>WDescdription text</Description> <!--Optional -->
					<Execute ExecType='If, If Not, Yes, No' ><!--Optional default is Yes -->
						<Rule Condition= 'variable' Condvalue='1' Else='next, abort, goto' Elsevalue='2'/>
					</Execute>
					<context>positive</context> <!-- values are positive and negative -->
					<Arguments><!-- user adds as many as needed. Optional -->
                            <argument name='count' value='1'/> 
                    </Arguments>
                    <onError action='next, abort, goto, abort_as_error' value='9'  /> <!-- step error handling, default is next
					value = used with action goto to specify step number -->
					<impact>noimpact</impact><!-- Used to decide if status of step will or will not impact 
					the testcase status. -->
               </step>
                <step Driver='DriverName'            Keyword='KeywordName'   />				
        </Steps>
</Testcase>
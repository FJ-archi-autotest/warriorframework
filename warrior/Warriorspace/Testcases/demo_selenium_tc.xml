<?xml version="1.0" ?>
<Testcase>
        <Details>
			<Name>demo_selenium_tc</Name>
			<Title>Test case to perform search action in google</Title>
			<ExpectedResults>PASS</ExpectedResults>
			<Category>search</Category>
			<Engineer>Sanika Kulkarni</Engineer>
			<Date/>
			<Time/>
			<default_onError action = 'next'  />
			<InputDataFile>../Data/demo_selenium_tc_Data.xml</InputDataFile>
			<Datatype>Custom</Datatype>
			<Logsdir/>
			<Resultsdir/>
        </Details>
       <Requirements>
       			<Requirement>requirement-001</Requirement>
       			<Requirement>requirement-002</Requirement>
       </Requirements>
        <Steps>
			<step TS= '1' Driver='selenium_driver' Keyword='browser_launch' > <!-- Will launch or open a new browser in window -->
				<Arguments>
					<argument name="system_name" value="search_element"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='browser_maximize' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='get_element_by_name' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="element_tag" value="name1"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='type_text' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="locator_type" value="name"/>
					<argument name="element_tag" value="name1"/>
					<argument name="text" value="python"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='send_keys_to_an_element' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="element_tag" value="name1"/>
					<argument name="locator_type" value="name"/>
					<argument name="text" value="RETURN"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='get_element_by_name' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="element_tag" value="name1"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='clear_text' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="locator_type" value="name"/>
					<argument name="element_tag" value="name1"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='get_element_by_name' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="element_tag" value="name1"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='type_text' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="element_tag" value="name1"/>
					<argument name="locator_type" value="name"/>
					<argument name="text" value="javascript"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='send_keys_to_an_element' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="element_tag" value="name1"/>
					<argument name="locator_type" value="name"/>
					<argument name="text" value="RETURN"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='get_element_by_id' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="element_tag" value="id1"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='click_an_element' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
					<argument name="element_tag" value="id1"/>
					<argument name="locator_type" value="id"/>
				</Arguments>
			</step>
			<step TS= '1' Driver='selenium_driver' Keyword='browser_close' >
				<Arguments>
					<argument name="system_name" value="search_element"/>
				</Arguments>
			</step>
		</Steps>
</Testcase>

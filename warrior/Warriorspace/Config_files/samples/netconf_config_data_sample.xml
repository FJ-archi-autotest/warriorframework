					</slot>
				<slot>
					<slotID>1</slotID>
					<cardType>PIT2-P2A1</cardType>
					<admin-status>up</admin-status>
				</slot>
				<slot>
					<slotID>2</slotID>
					<cardType>PIT2-P2A1</cardType>
					<admin-status>up</admin-status>
				</slot>
			</shelf>
			</eqpt>
		</config>       
    </config_data>
    
    <config_data name='theirConfigFile'>
        <filepath>../../Config_files/netconf_myconfig_sample.xml</filepath>
    </config_data>
    
    
</configs>


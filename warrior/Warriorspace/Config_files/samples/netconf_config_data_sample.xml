<?xml version="1.0" ?>
<configs>
    <config_data name='myConfig'>
		<config>
			<interfaces xmlns="urn:ietf:params:xml:ns:yang:ietf-interfaces">
				<interface>
					<name>ots-2/0/0/E1</name>
					<type xmlns:iana-interface-type="urn:ietf:params:xml:ns:yang:iana-if-type">:iana-interface-type:opticalTransport</type>
					<ots xmlns="urn:fujitsu:params:xml:ns:yang:interface:ot">
						<admin-status>up</admin-status>
						<oms>
							<postoutadj>1</postoutadj>
							<och>
								<och-id>1</och-id>
								<center-frequency>196.1</center-frequency>
								<slot-width>50</slot-width>
							</och>
						</oms>
					</ots>
				</interface>
			</interfaces>

<?xml version="1.0" ?>

<configs>
    <config_data name='myConfig'>
	<config>
    <eqpt xmlns="urn:fujitsu:params:xml:ns:yang:equipment">
		<shelf>
			<shelfId>${shelf_global.value}</shelfId>
			<slot>
				<slotID>${network1.ne1.shelf1.slot1.id}</slotID>
				<subslot>
					<subslotID>${network1.ne1.shelf1.slot1.subslot1.id}</subslotID>
					<port>
						<portID>${network1.ne1.shelf1.slot1.subslot1.port.port1}</portID>
						<pluggableInterfaceType>${network1.ne1.shelf1.slot1.subslot1.pint.id}</pluggableInterfaceType>
						<admin-status>${network1.ne1.shelf1.slot1.subslot1.admin.state1}</admin-status>
					</port>
				</subslot>
			</slot>	
			<slot>
				<slotID>${network1.ne1.shelf1.slot1.id1}</slotID>
				<cardType>PIT2-P2A1</cardType>
				<admin-status>up</admin-status>
			</slot>
			<slot>
				<slotID>${network1.ne1.shelf1.slot1.id2}</slotID>
				<cardType>PIT2-P2A1</cardType>
				<admin-status>up</admin-status>
			</slot>
		</shelf>
	</eqpt>	
    </config>
    </config_data>

    <config_data name='myConfig1'>
	<config>
    <eqpt xmlns="urn:fujitsu:params:xml:ns:yang:equipment">
		<shelf>
			<shelfId>1</shelfId>
			<slot>
				<slotID>0</slotID>
				<subslot>
					<subslotID>0</subslotID>
					<port>
						<portID>C2</portID>
						<pluggableInterfaceType>CFP4</pluggableInterfaceType>
						<admin-status>up</admin-status>
					</port>
				</subslot>
			</slot>				
		</shelf>
	</eqpt>	
    </config>
    </config_data>

	<config_data name='myConfigFile1'>
        <filepath>../../Config_files/samples/netconf_myconfig_sample2.xml</filepath>
	</config_data>

	<config_data name='myConfigFile2'>
        <filepath>../../Config_files/samples/netconf_myconfig_sample3.xml</filepath>
        <filepath>../../Config_files/samples/netconf_myconfig_sample4.xml</filepath>
	</config_data>

	
</configs> 




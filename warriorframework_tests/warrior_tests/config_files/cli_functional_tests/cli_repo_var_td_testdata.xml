<?xml version="1.0" ?>

<data>

	<global>
		<command_params
	        start=".*" end="\$" timeout="" sleep="2" >
		</command_params>
		<verifications>
		</verifications>
	</global>

	<testdata execute="yes" title ="td_with_repo_valid" >
		<command send="${REPO.ls_cmd}${REPO.ls_opt}"
		         verify="v1,v2" resp_ref="lsout" resp_pat_req="[\d]+"/>
		<v1 found="no"  search="${REPO.v1} NOT_FOUND" />
		<v2 found="yes" search="${REPO.v2}" />
		<v3 found="yes" search="${v3.value}" />
		<v4 found="yes" search="$wdf{rtxttalp01_no_subsys.username}"/>
	</testdata>

	<!-- Use repo variable which is not available -->
	<testdata execute="yes" title ="td_with_repo_invalid" >
		<command send="${REPO.ls_cmd}${REPO.unknown}"
		         verify="v1,v2" />
		<v1 found="no"  search="${REPO.unknown} NOT_FOUND" />
		<v2 found="yes" search="${REPO.v2}" />
		<v3 found="yes" search="${v3.value}" />
	</testdata>
</data>
